{
  "name": "url-shortener",
  "version": "1.0.0",
  "description": "A simple SHORTENER written in Typescript, Node.js, Express.js, MongoDB and React.js",
  "main": "server.ts",
  "scripts": {
    "build-ts": "tsc",
    "build": "npm run build-ts",
    "start": "node server.ts",
    "dev": "nodemon server.ts",
    "test": "node --watch . --ext ts --exec \"mocha --timeout 30000 -r ts-node/register test/**/*.test.ts\"",
    "watch-ts": "tsc -w",
    "watch-test": "npm run test -- --watchAll",
    "test-dev": "nodemon --watch . --ext ts --exec \"mocha --timeout 30000 -r ts-node/register test/**/*.test.ts\""
  },
  "keywords": [
    "Node.js",
    "MongoDB",
    "Express.js",
    "URL Shortener",
    "Long URL to Short URL",
    "Typescript",
    "NoSQL"
  ],
  "author": "Md Sazzadul Islam",
  "license": "ISC",
  "homepage": "https://github.com/MdSazzadIslam/url-Shortener",
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/chai-http": "^4.2.0",
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/mocha": "^9.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.6.2",
    "@types/redis": "^2.8.31",
    "@types/supertest": "^2.0.11",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "helmet": "^4.6.0",
    "mocha": "^9.1.0",
    "mongoose": "^5.13.7",
    "nanoid": "^3.1.25",
    "redis": "^3.1.2",
    "supertest": "^6.1.6",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  }
}
